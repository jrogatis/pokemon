{
  "name": "pokedex",
  "version": "0.1.0",
  "description": "a simple pokemon list exemple",
  "author": "Jean Philip de Rogatis",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "8.11.1"
  },
  "dependencies": {
    "@material-ui/core": "^1.0.0-rc.1",
    "@material-ui/icons": "^1.0.0-rc.0",
    "@vx/group": "0.0.161",
    "@vx/mock-data": "0.0.161",
    "@vx/responsive": "0.0.161",
    "@vx/scale": "0.0.161",
    "@vx/shape": "0.0.162",
    "autoprefixer": "^8.4.1",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.1",
    "chart.js": "^2.7.2",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "dotenv-expand": "4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.11",
    "font-loader": "^0.1.2",
    "fs-extra": "^6.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "^22.4.3",
    "lodash": "^4.17.10",
    "material-ui": "^1.0.0-beta.47",
    "mdi-material-ui": "^4.0.0",
    "moment": "^2.22.1",
    "numeral": "^2.0.6",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.5",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "rc-slider": "^8.6.1",
    "react": "^16.3.2",
    "react-chartkick": "^0.2.0",
    "react-container-dimensions": "^1.3.3",
    "react-dev-utils": "^5.0.1",
    "react-device-detect": "^1.3.4",
    "react-dom": "^16.3.2",
    "react-placeholder": "^3.0.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^4.0.0",
    "redux-persist": "^5.9.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "resolve": "^1.7.1",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "^1.0.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.4.1",
    "redux-logger": "^3.0.6"
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "npm run fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --coverage",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint-verbose": "DEBUG=eslint:cli-engine eslint .",
    "lint": "npx eslint .",
    "fix": "npx eslint . --fix",
    "prettier": "npx prettier --config ./.prettierrc --write './**/*.js'"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "./src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
